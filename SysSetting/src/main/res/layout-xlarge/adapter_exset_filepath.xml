<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <LinearLayout
        style="@style/WmHw"
        android:layout_marginBottom="1dp"
        android:background="@drawable/normal_bt_selector"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/id_exset_img_file_ico"
            style="@style/WwHw"
            android:layout_marginLeft="@dimen/ds_little"
            android:layout_marginRight="@dimen/ds_little"
            android:src="@drawable/path_back_selector"
            />
        <!--android:src="@drawable/scan_folder_icon_pressed"-->

        <TextView
            android:id="@+id/id_exset_text_file_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:textColor="@color/col_white"
            android:textSize="@dimen/ds_normal_text_size_little"
            />

    </LinearLayout>

</LinearLayout>
