<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_focused="true">
        <shape>
            <corners android:radius="3dp"></corners>

            <stroke android:width="4dp" android:color="#DD2d7faa"></stroke>
            <solid android:color="#7099d7f8" />
        </shape>
    </item>
    <item android:drawable="@color/col_menu_bg_focus" android:state_checked="true">
    </item>
    <item android:drawable="@color/col_menu_bg" android:state_checked="false"></item>

</selector>
